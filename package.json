{
  "name": "generate-project-template",
  "version": "0.0.10",
  "description": "Build your own custom project templates with this project template generator from anywhere on your machine.",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:test": "node dist/index.js",
    "build": "tsc && shx rm -rf dist/templates && shx cp -r src/templates dist",
    "clean": "shx rm -rf ./dist/",
    "version": "npm version",
    "pack": "npm pack",
    "publish": "build && npm publish && clean"
  },
  "keywords": [],
  "author": "Mikolaj Adamowicz",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/ejs": "^3.0.2",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^13.11.1",
    "@types/shelljs": "^0.8.7",
    "nodemon": "^2.0.3",
    "shx": "^0.3.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "ejs": "^3.1.6",
    "inquirer": "^8.2.2",
    "shelljs": "^0.8.5"
  }
}
